<%- include('./partials/header.ejs') %>
<%- include('./partials/navbar.ejs') %>
<div class="container">
    <div class="row mt-4">
<h1> Login Page </h1>
<hr>
    <form method="post">
        <div class="mb-3">
            <label for="email" class="form-label">Email Address</label>
            <input type="email" class="from-control" required id="email" aria-describedby="emailhelp" name="email" />
        </div>     
        <div class="mb-3">
            <label for="password" class="form-label">Password</label>
            <input type="password" class="from-control" required id="paasword" name="password" />
        </div>
        <button type="submit" class="btn btn-primary">Login</button>
    </form>
    </div>
</div>
<%- include('./partials/footer.ejs') %>